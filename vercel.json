{
  "rewrites": [
    {
      "source": "/h.html",
      "destination": "/h.html"
    },
    {
      "source": "/h",
      "destination": "/h.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}